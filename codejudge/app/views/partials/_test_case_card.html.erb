<div class="test-case-card <%= passed ? "test-case-card--success" : "test-case-card--failed" %>">
  <div class="test-case-card__status">
    <% if passed %>
      <p>Passed</p>
    <% else %>
      <p>Failed</p>
    <% end %>
  </div>
  <span class="test-case-card__separator"></span>
  <p class="test-case-card__copy">This is a sample test case<%=  %></p>
</div>